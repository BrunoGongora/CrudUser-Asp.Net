@model Crud_Asp.Net.Models.Contacto

<style>
    .btn_back{
        width: 10rem;
    }

    .card-header {
        background-color: #505050;
    }
</style>

<h1 class="text-center my-4">Detalle Usuario</h1>

<div class="row">
    <div class="offset-12">
        <div class="card border">
            <div class="card-header">
                <h3 class="text-white"> <strong>Detalle</strong></h3>
            </div>
            <div class="card-body">
                <ul class="list-group">
                    <li class="list-group-item"><strong>Nombre: </strong>@Html.DisplayFor(m => m.Nombre)</li>
                    <li class="list-group-item"><strong>Telefono: </strong>@Html.DisplayFor(m => m.Telefono)</li>
                    <li class="list-group-item"><strong>Dirección: </strong>@Html.DisplayFor(m => m.Direccion)</li>
                    <li class="list-group-item"><strong>Email: </strong>@Html.DisplayFor(m => m.Email)</li>
                    <li class="list-group-item"><strong>Fecha de Registro: </strong>@Html.DisplayFor(m => m.FechaRegistro)</li>
                </ul>
            </div>
            
        </div>
        
    </div>
    <div class="btn_back my-2">
        <a class="btn btn-secondary my-4" asp-controller="Home" asp-action="Index">Volver</a>
    </div>
    
</div>


